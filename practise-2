n = int(input('enter a number:'))
d=dict()
for i in range(1, n+1):
    d[i]=i*i
print(d)


list = eval(input('enter a number:'))
l=[]
for i in range(1, list):
    l.append(i)
print(l)
t1=tuple(l)
print(t1)



import math
n=int(input('enter a number:'))
a=50
h=30
l=[]
b=[x for x in range(n)]
for d in b:
    l.append(str(int(round(math_sqrt(2*a*float(d)/h)))))

print(l)    
    

    
    
